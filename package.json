{
  "name": "csc-front-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Angel",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:serve": "concurrently \"cross-env BROWSER=none npm run start\" \"npm run electron:start\"",
    "electron:build": "npm run build && electron-builder --dir -c.extraMetadata.main=X:/REACT/electron-react/build/electron.js",
    "electron:start": "wait-on tcp:5173 && electron ."
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@tanstack/react-table": "^8.15.3",
    "axios": "^1.6.8",
    "chart.js": "^4.4.2",
    "electron-is-dev": "^3.0.1",
    "electron-remote": "^1.3.0",
    "formik": "^2.4.5",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3",
    "react-select": "^5.8.0",
    "sass": "^1.71.1",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.5",
    "sonner": "^1.5.0",
    "sweetalert2": "^11.12.0",
    "yup": "^1.4.0",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
